{
  "name": "ts-react-redux-starter",
  "version": "1.0.0",
  "description": "A complete project starter kit for a TypeScript React/Redux application with full support for code splitting and no hidden configs.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rudfoss/ts-react-redux-starter.git"
  },
  "keywords": [
    "react",
    "redux",
    "starter",
    "starter kit",
    "redux saga",
    "code splitting",
    "webpack"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rudfoss/ts-react-redux-starter/issues"
  },
  "homepage": "https://github.com/rudfoss/ts-react-redux-starter#readme",
  "scripts": {
    "dev": "rimraf ./dist-dev && cross-env TS_NODE_PROJECT=webpack/tsconfig.json webpack-dev-server --config webpack/webpack.dev.ts --progress --hot",
    "start": "npm run dev",
    "build:analyze": "rimraf ./dist-dev && cross-env TS_NODE_PROJECT=webpack/tsconfig.json webpack --config webpack/webpack.analyze.ts --progress",
    "build": "rimraf ./dist && cross-env TS_NODE_PROJECT=webpack/tsconfig.json webpack --config webpack/webpack.prod.ts --progress",
    "test": "jest",
    "test:watch": "jest --watch",
    "serve": "ws --port 3000 --https --directory ./dist --spa ./index.html",
    "lint": "eslint src/** && eslint webpack/**"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/autoprefixer": "^9.7.2",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.5",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/jest": "^25.2.1",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/react-redux": "^7.1.7",
    "@types/redux-actions": "^2.6.1",
    "@types/terser-webpack-plugin": "^2.2.0",
    "@types/webpack": "^4.41.12",
    "@types/webpack-bundle-analyzer": "^2.13.3",
    "@types/webpack-dev-server": "^3.10.1",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-plugin": "^4.2.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.5.1",
    "local-web-server": "^4.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "react-hot-loader": "^4.12.20",
    "rimraf": "^3.0.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^2.3.6",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
